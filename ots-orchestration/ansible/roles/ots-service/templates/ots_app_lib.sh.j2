#!/bin/bash -e

PWD="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

function ots_stop(){
	pid=$(ots_pid)
	$pid | while read line
	do
		kill -9 $line
	done
}

function ots_start(){
	java -jar /$PWD/{{ ots_binary }}
}

function ots_pid(){
	echo `ps aux | grep ots-service |grep -v grep| tr -s ' '|cut -f2 -d ' '`
}

